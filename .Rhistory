tmpfun <- function(x,df=1) {
hist(x, prob=TRUE)
curve( dchisq(x, df=df), col='blue', add=TRUE )
}
tmpfun(1)
tmpfun(5)
tmpfun(5)
tmpfun(1:10, 5)
tmpfun(1000, 5)
tmpfun(1:1000, 5)
tmpfun(1:100, 5)
